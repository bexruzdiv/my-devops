{% if minio.enabled %}
MINIO_ACCESS_KEY={{ minio.access_key }}
MINIO_SECRET_KEY={{ minio.secret_key }}
{% endif %}
{% if rmq.enabled %}
RMQ_USERNAME={{ rmq.user }}
RMQ_PASSWORD={{ rmq.pass }}
{% endif %}
{% if mongo_docker.enabled %}
MONGO_INITDB_ROOT_USERNAME={{ mongo_docker.user }}
MONGO_INITDB_ROOT_PASSWORD={{ mongo_docker.pass }}
{% endif %}
{% if redis.enabled %}
REDIS_PASSWORD={{ redis.pass }}
{% endif %}
{% for envs in dotenv_vars %}
{{ envs }}
{% endfor %}